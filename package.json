{
  "name": "express-starter-with-ts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "tsc-watch --onSuccess \"npm run start:debug\" --onFailure \"echo Beep! Compilation Failed\"",
    "start:debug": "cross-env NODE_ENV=development DEBUG=express-starter-with-ts:* node dist/index.js",
    "build": "rm -r dist && tsc",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "less-middleware": "^3.1.0",
    "module-alias": "^2.2.0",
    "moment-timezone": "^0.5.25",
    "mongoose": "^5.5.11",
    "morgan": "~1.9.1",
    "pug": "^2.0.3",
    "rotating-file-stream": "^1.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/mongoose": "^5.5.3",
    "@types/node": "^12.0.2",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.4.5"
  }
}
